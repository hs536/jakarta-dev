    <toolchain>
        <type>jdk</type>
        <provides>
            <version>11</version>
        </provides>
        <configuration>
            <jdkHome>/usr/lib/jvm/jdk11</jdkHome>
        </configuration>
    </toolchain>
</toolchains>
